<%- include('../partials/header') %>
<h2>Blog Posts</h2>
<% blogs.forEach(blog => { %>
    <div>
        <h3><%= blog.title %></h3>
        <p><%= blog.content %></p>
        <% if (blog.image) { %>
            <img src='<%= blog.image %>' style='width:100px;'/><% } %>
            <hr>
    </div><% }) %>
<%- include('../partials/footer') %>